$(document).ready(function(){

   // load the json directly from the api
   $.ajax({
          url: 'api.php?id=' + license_id,
          dataType: 'json',
          success: function(data){
          	alert(1);
            // update the KelpKSONView
            jQuery.JSONView(data, $('#json-wrapper').empty());
            alert(2);
          }
        });
 });