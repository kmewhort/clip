<!-- content -->
<div id="content-outer" class="clear">
  <div id="content-wrap">
    <div id="content">
      <div id="left">
        <% licence_info_categories(@licence).each do |key,str| %>
            <div class="entry">
              <h3 class="licence-info-category"><%=  content_tag :a, str, name: key %></h3>
              <%= render partial: key.to_s + "/show", locals: { licence: @licence } %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

<!-- sidebar -->
<div id="right">
  <div class="sidemenu">
    <h3>Go to</h3>
    <ul>
      <% licence_info_categories(@licence).each do |key, str| %>
          <li>
            <%= link_to str, "#" + key.to_s %>
          </li>
      <% end %>
    </ul>
  </div>
</div>